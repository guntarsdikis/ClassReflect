PORT=3000
NODE_ENV=development
FRONTEND_URL=https://classreflect.gdwd.co.uk

# Database
DB_HOST=gdwd.cluster-cjjl7f5jormj.eu-west-2.rds.amazonaws.com
DB_PORT=3306
DB_NAME=classreflect
DB_USER=
DB_PASSWORD=

# AWS
AWS_REGION=eu-west-2
AWS_SES_REGION=eu-west-1
SES_FROM_EMAIL=info@gdwd.co.uk
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
SQS_QUEUE_URL=

# Authentication
JWT_SECRET=

# Processing
ASSEMBLYAI_API_KEY=

# OpenAI (when ANALYSIS_PROVIDER=openai)
OPENAI_MODEL=gpt-4o

# Gemini (when ANALYSIS_PROVIDER=gemini)
GOOGLE_API_KEY=
GEMINI_MODEL=gemini-1.5-pro
GEMINI_USE_JSON_MIME=true
GEMINI_MAX_OUTPUT_TOKENS=16000
GEMINI_CRITERIA_BATCH_SIZE=3
GEMINI_TEMPERATURE=0.6
GEMINI_TOP_K=40
GEMINI_TOP_P=0.95
GEMINI_FORCE_SINGLE_PROMPT=false
GEMINI_DISABLE_FALLBACK=false
GEMINI_OUTPUT_MODE=minimal # minimal | rich
# Vertex AI (optional, when ANALYSIS_PROVIDER=vertex)
VERTEX_API_KEY=
VERTEX_PROJECT_ID=
VERTEX_LOCATION=us-central1
VERTEX_MODEL=gemini-1.5-pro

# OpenRouter (unified API for all models - recommended)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=
OPENROUTER_MODEL=google/gemini-pro-1.5
OPENROUTER_TEMPERATURE=0.6
OPENROUTER_MAX_TOKENS=16384
OPENROUTER_BATCH_SIZE=3
OPENROUTER_FORCE_SINGLE_PROMPT=false
OPENROUTER_DISABLE_FALLBACK=false
OPENROUTER_OUTPUT_MODE=minimal
